{
  "name": "react-project-ui",
  "version": "0.2.11",
  "description": "",
  "keywords": [],
  "author": "evgnyx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evgnyx/react-project-ui.git"
  },
  "bugs": {
    "url": "https://github.com/evgnyx/react-project-ui/issues"
  },
  "homepage": "https://github.com/evgnyx/react-project-ui#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf dist",
    "build": "rollup -c && npm run types && npm run copy",
    "types": "tsc --emitDeclarationOnly",
    "css": "postcss src/**/*.less --watch --dir dist/css --base src --ext css",
    "copy": "node scripts/build",
    "start": "rollup -c -w",
    "prepare": "npm run build && npm run copy",
    "version": "git add -A src",
    "postversion": "npm run copy && git push && git push --tags",
    "publish": "cd dist && npm publish"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@rollup/plugin-babel": "^5.0.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@rollup/plugin-typescript": "^4.1.2",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "rimraf": "^3.0.2",
    "rollup": "^2.17.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
